<Application x:Class="Projeto_Educa_Sonho_Meu.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Projeto_Educa_Sonho_Meu"
             StartupUri="Paginas/TeladeLogin.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <Style x:Key="LabelBorder" TargetType="Border">
                <Setter Property="Padding" Value="5"></Setter>
                <Setter Property="Margin" Value="0 5"></Setter>
                <Setter Property="CornerRadius" Value="0 10 0 10"></Setter>
                <Setter Property="Background" Value="#d0ffff"></Setter>
            </Style>

            <Style x:Key="LabelText" TargetType="TextBlock">
                <Setter Property="FontSize" Value="18"/>
                <Setter Property="FontWeight" Value="SemiBold"/>
            </Style>
        </ResourceDictionary>
    </Application.Resources>
</Application>
